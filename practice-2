 struct Stu
{
    char name[10];
    int age;
};
void input(struct Stu* p);
void output(struct Stu* p);
int main()
{
    struct Stu arr[5];
    input(arr);
    output(arr);
    return 0;
}
void input(struct Stu* p)
{
    int i=0;
    for(i=0;i<5;i++)
    {
        scanf(" %s %d",p[i].name,&(p[i].age));
    }
}
void output(struct Stu* p)
{
    int i=0;
    for(i=0;i<5;i++)

    {
        printf("%s %d\n",p[i].name,p[i].age);
    }

}
